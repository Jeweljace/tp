@startuml
title Parsing command (Parser constructs DeleteCommand)

skinparam shadowing false
skinparam defaultFontName Arial
skinparam sequenceMessageAlign center

actor User
participant ":Ui" as Ui #E3F2FD
participant ":Parser" as Par #E8F5E9
participant ":DeleteCommand" as Del #E8F5E9

User -> Ui : enter "delete 2"
activate Ui
Ui -> Par : parse("delete 2")
activate Par

create Del
Par -> Del ++ : DeleteCommand(2)

' Return the command object to UI
Par -->> Ui : Command (DeleteCommand)

deactivate Par
deactivate Ui
@enduml
